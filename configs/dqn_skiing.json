{
  "agent_type": "dqn",
  "env": {
    "id": "ALE/Skiing-v5",
    "seed": 42,
    "kwargs": {},
    "use_skiing_tracker": true
  },
  "model": {
    "input_shape": [4, 84, 84],
    "network_type": "dueling"
  },
  "train": {
    "learning_rate": 0.0001,
    "use_rmsprop": true,
    "gamma": 0.99,
    "epsilon_start": 1.0,
    "epsilon_end": 0.01,
    "epsilon_decay": 10000,
    "memory_size": 1000000,
    "batch_size": 32,
    "target_update": 1000,
    "num_episodes": 2000,
    "render_every": 5,
    "respect_env_done": true,
    "max_steps_per_episode": 5000,
    "log_each_reward": false,
    "reward_log_every": 100,
    "reset_on_crash": false,
    "crash_reward_threshold": -100.0,
    "save_every": 100,
    "checkpoint_path": "checkpoints/dqn_skiing_ep{EPISODE}.pth",
    "log_dir": "runs/dqn_skiing",
    "stats_window": 100,
    "reward_clip": null,
    "warmup_steps": 5000
  },
  "display_reward": {
    "mode": "negate",
    "scale": 1.0,
    "shift": 0.0
  }
}


