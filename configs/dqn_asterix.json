{
  "env": {
    "id": "ALE/Asterix-v5",
    "kwargs": {},
    "seed": 42
  },
  "train": {
    "num_episodes": 1000,
    "learning_rate": 2.5e-4,
    "use_rmsprop": true,
    "batch_size": 32,
    "gamma": 0.99,
    "epsilon_start": 1.0,
    "epsilon_end": 0.01,
    "epsilon_decay": 1000000,
    "target_update": 10000,
    "memory_size": 1000000,
    "warmup_steps": 1000,
    "reward_clip": 1.0,
    "save_every": 100,
    "render_every": 0,
    "max_steps_per_episode": null,
    "stats_window": 100,
    "log_dir": "runs/dqn_asterix"
  },
  "model": {
    "network_type": "rainbow",
    "input_shape": [4, 84, 84],
    "n_atoms": 51,
    "v_min": -10,
    "v_max": 10,
    "noisy": true
  }
}