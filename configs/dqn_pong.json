{
  "env": {
    "id": "ALE/Pong-v5",
    "kwargs": {},
    "seed": 42
  },
  "train": {
    "num_episodes": 1000,
    "learning_rate": 2.5e-4,
    "use_rmsprop": true,
    "rmsprop_alpha": 0.95,
    "rmsprop_momentum": 0.95,
    "rmsprop_eps": 0.01,
    "gradient_clip": 10.0,
    "batch_size": 32,
    "gamma": 0.99,
    "epsilon_start": 1.0,
    "epsilon_end": 0.01,
    "epsilon_decay": 1000000,
    "target_update": 10000,
    "memory_size": 1000000,
    "warmup_steps": 1000,
    "reward_clip": 1.0,
    "save_every": 100,
    "render_every": 1,
    "max_steps_per_episode": null,
    "stats_window": 100,
    "checkpoint_path": "checkpoints/dqn_pong_dueling_ep{EPISODE}.pth",
    "checkpoint_dir": "checkpoints",
    "runs_dir": "runs",
    "log_dir": "runs/dqn_pong"
  },
  "model": {
    "network_type": "dueling",
    "input_shape": [4, 84, 84]
  }
}