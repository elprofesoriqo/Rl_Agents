{
  "agent_type": "dqn",
  "env": {
    "id": "ALE/Skiing-v5",
    "kwargs": {},
    "seed": 42
  },
  "train": {
    "learning_rate": 0.0001,
    "use_rmsprop": true,
    "gamma": 0.99,
    "epsilon_start": 0.0,
    "epsilon_end": 0.0,
